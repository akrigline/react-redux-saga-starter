{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "recompose": "^0.23.5",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.5",
    "@storybook/addon-storyshots": "^3.1.6",
    "@storybook/addons": "^3.1.6",
    "@storybook/channels": "^3.1.6",
    "@storybook/react": "3.1.6",
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "empty": "^0.10.1",
    "enzyme": "^2.9.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "find-cache-dir": "^1.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "pre-commit": "^1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.2.1",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "npm run test -- --coverage",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.js src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/containers/**/*.js",
      "src/routes/**/*.js",
      "src/redux/reducers/**/*.js",
      "src/redux/sagas/**/*.js",
      "!**/*.story.js",
      "!**/stories.js",
      "!**/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "blueprints/",
      "build/",
      ".*\\.json",
      "__snapshots__/",
      "src/__tests__/shoryshots.test.js",
      "stories.js",
      "src/services",
      "story.js",
      "src/routes/root",
      "src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": ["react-app"],
    "plugins": ["react-hot-loader/babel"]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": ["lint", "test", "coverage"]
}
